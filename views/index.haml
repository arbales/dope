.posts
  - @posts.each do |post|
    .post.deletable{:'data-id' => post.id}
      .image               
        = image_tag post.image.url
      .meta     
        #{post.yeps} yeps. #{post.nopes} nopes.
        .author
          = link_to post.username, "/user/#{post.username}"

.clear.vspace      
.grid_12
  = button_tag "Upload", :class => 'new-upload'
  